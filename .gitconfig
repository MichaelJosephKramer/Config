[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  last = cat-file commit HEAD
[color]
  ui = true
[user]
	name = Michael Joseph Kramer
	email = michael.kramer@live.com
[core]
	editor = /usr/bin/vim
[merge]
	tool = p4merge
	keepBackup = false
[mergetool "p4merge"]
	cmd = p4merge "$BASE" "$LOCAL" "$REMOTE" "$MERGED"
  trustExitCode = false
  keepTemporaries = false
  keepBackup = false
[diff]
  external = p4diff
